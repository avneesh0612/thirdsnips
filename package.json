{
  "name": "thirdweb-snippets",
  "version": "0.1.0",
  "repository": "git@github.com:avneesh0612/thirdweb-snippets.git",
  "author": "Avneesh Agarwal",
  "license": "GPL-3.0-only",
  "scripts": {
    "install": "yarn install && cd src/automation && yarn install",
    "generateSnippets": "node ./src/automation/scripts/generateSnippets.js",
    "generateReadme": "node ./src/automation/scripts/generateReadme.js",
    "prepare": "husky install",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "pre-commit": "yarn run generateReadme && yarn run generateSnippets && yarn run format && git add -A ."
  },
  "devDependencies": {
    "commitlint": "^16.2.3",
    "commitlint-config-gitmoji": "^2.2.5",
    "husky": "^7.0.4",
    "prettier": "^2.6.0"
  },
  "dependencies": {
    "@commitlint/cli": "^16.2.3",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/react.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/react.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/sdk.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/sdk.json"
      }
    ]
  }
}
