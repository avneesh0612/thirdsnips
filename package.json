{
  "name": "thirdweb-snippets",
  "version": "0.0.1",
  "repository": "git@github.com:avneesh0612/thirdweb-snippets.git",
  "author": "Avneesh Agarwal",
  "license": "GPL-3.0-only",
  "scripts": {
    "generateReadme": "node ./src/automation/scripts/generateReadme.js",
    "prepare": "husky install",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "pre-commit": "yarn run generateReadme && yarn run format && git add -A .",
    "start:web": "cd src/web && yarn install && yarn start"
  },
  "devDependencies": {
    "commitlint": "^16.2.3",
    "commitlint-config-gitmoji": "^2.2.5",
    "husky": "^7.0.4",
    "prettier": "^2.6.0"
  },
  "dependencies": {
    "@commitlint/cli": "^16.2.3"
  },
  "contributes": {
    "commands": [
      {
        "command": "thirdweb.helloWorld",
        "title": "Hello World"
      }
    ],
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/bundle-collection.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/bundle-collection.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/token.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/token.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/nft-collection.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/nft-collection.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/vote-collection.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/vote-collection.json"
      }
    ]
  }
}
