{
  "name": "thirdweb-snippets",
  "displayName": "Thirdweb Snippets",
  "description": "This is an extension for Thirdweb Snippets with Typescript support as well!",
  "version": "0.3.0",
  "publisher": "AvneeshAgarwal",
  "repository": {
    "url": "https://github.com/avneesh0612/thirdweb-snippets"
  },
  "engines": {
    "vscode": "^1.64.0"
  },
  "scripts": {
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "format": "prettier --write '**/*.{ts,json,md,js}'"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/nft-collection.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/nft-collection.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/token.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/token.json"
      }
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "prettier": "^2.5.1"
  }
}