{
  "name": "thirdweb-snippets",
  "displayName": "Thirdweb Snippets",
  "description": "Stop re-writing thirdweb snippets. Use thirdsnips to make it all snap!",
  "version": "0.0.2",
  "publisher": "Avneesh Agarwal",
  "repository": {
    "url": "https://github.com/avneesh0612/thirdweb-snippets"
  },
  "engines": {
    "vscode": "^1.65.0"
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "concurrently \"rollup -c\" \"tsc -p ./\"",
    "watch": "concurrently \"rollup -c -w\" \"tsc -watch -p ./\"",
    "lint": "eslint .",
    "format": "prettier --write .",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:thirdweb.nft"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "thirdweb.nft",
        "title": "Early access NFT",
        "category": "thirdsnips"
      }
    ],
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/bundle-collection.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/bundle-collection.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/token.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/token.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/nft-collection.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/nft-collection.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/vote-collection.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/vote-collection.json"
      }
    ]
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.1",
    "@tsconfig/svelte": "^3.0.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.65.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vscode/test-electron": "^2.0.3",
    "concurrently": "^7.0.0",
    "eslint": "^8.6.0",
    "glob": "^7.2.0",
    "mocha": "^9.1.3",
    "prettier": "^2.6.0",
    "rollup": "^2.70.1",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.46.4",
    "svelte-check": "^2.4.6",
    "svelte-preprocess": "^4.10.4",
    "typescript": "^4.5.4"
  }
}
